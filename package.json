{
  "name": "serial-api-examples",
  "version": "0.0.1",
  "description": "Using the IMU 9-axis sensor to plot on webgl-plot",
  "main": "index.js",
  "dependencies": {
    "webgl-plot": "^0.2.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "rollup": "^1.27.14",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "build1": "tsc Counter/src/Counter.ts --outFile Counter/dist/Counter.js --sourceMap true --typeRoots @types",
    "build2": "tsc -p ./IMU-Text && rollup --config ./IMU-Text/rollup.config.js",
    "build3": "tsc -p ./IMU-Text-Plot && rollup --config ./IMU-Text-Plot/rollup.config.js",
    "build": "npm run build1 && npm run build2 && npm run build3",
    "ver": "tsc --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danchitnis/Serial-API-Examples.git"
  },
  "author": "Danial Chitnis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danchitnis/Serial-API-Examples/issues"
  },
  "homepage": "https://github.com/danchitnis/Serial-API-Examples#readme"
}
