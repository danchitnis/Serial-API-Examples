{
  "name": "serial-api-examples",
  "version": "0.0.1",
  "description": "Using the IMU 9-axis sensor to plot on webgl-plot",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "build1": "tsc Counter/src/Counter.ts --outFile Counter/dist/Counter.js --sourceMap true --typeRoots @types",
    "build2": "tsc IMU-Text/src/IMU-Text.ts --outFile IMU-Text/dist/IMU-Text.js --sourceMap true --typeRoots @types --target ES2015",
    "build3": "tsc IMU-Text-Plot/src/IMU-Text-Plot.ts --outFile IMU-Text-Plot/dist/IMU-Text-Plot.js --sourceMap true --typeRoots @types --target ES2015",
    "build": "npm run build1 && npm run build2 && npm run build3",
    "ver": "tsc --version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danchitnis/Serial-API-Examples.git"
  },
  "author": "Danial Chitnis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danchitnis/Serial-API-Examples/issues"
  },
  "homepage": "https://github.com/danchitnis/Serial-API-Examples#readme"
}
